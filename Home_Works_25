// Задача 25: Напишите цикл, который принимает на вход два числа (A и B) и возводит число A в натуральную степень B.
// 3, 5 -> 243 (3⁵)
// 2, 4 -> 16
int funPow(int n, int x)
{
    if (n <= 0 || x <= 0)
    {
        return 0;
    }
    else
    {
        int y = 1;
        for (int i = 0; i < x; i++)
        {
            y *= n;

        }
        return y;
    };

}
